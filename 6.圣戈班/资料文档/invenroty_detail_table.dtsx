        <SQLTask:SqlTaskData
          SQLTask:Connection="{80C1EBC5-ABD2-4A8B-B493-8DAF6ACDB080}"
          SQLTask:SqlStatementSource="update inventory_detail_table set sbu=a.[大SBU],sub_sbu=a.[小SBU],category=a.category&#xA;from &#xA;(select * from inventory_category_mapping&#xA;where inventory_category_mapping.comments2 like '%prod%') as a &#xA;where inventory_detail_table.product_line = a.[Prod Line/cost center]&#xA;and inventory_detail_table.domain = a.Domain;&#xA;&#xA;------&#xA;update inventory_detail_table set sbu=a.[大SBU],sub_sbu=a.[小SBU]&#xA;from &#xA;(select * from inventory_category_mapping&#xA;where inventory_category_mapping.comments2 =N'通过cost center识别大小SBU') as a &#xA;where inventory_detail_table.[cost_center]= a.[Prod Line/cost center]&#xA;and inventory_detail_table.domain = a.Domain;&#xA;&#xA;&#xA;-----&#xA;update inventory_detail_table set sbu=a.[大SBU],sub_sbu=a.[小SBU],category=a.category&#xA;from &#xA;(select * from inventory_category_mapping&#xA;where inventory_category_mapping.comments2 = N'通过cost center识别大小SBU和category') as a &#xA;where inventory_detail_table.[cost_center] = a.[Prod Line/cost center]&#xA;and inventory_detail_table.domain = a.Domain;&#xA;&#xA;------&#xA;update inventory_detail_table set sbu=a.[大SBU],sub_sbu=a.[小SBU],category=&#xA;case&#xA;&#x9;when inventory_detail_table.item_desc_1 like N'%C-FLEX%' then 'C-FLEX US'&#xA;&#x9;when inventory_detail_table.item_desc_1 like 'N%374-%' then 'C-FLEX IN'&#xA;&#x9;when inventory_detail_table.item_desc_1 like N'%ABW%' then 'ABW'&#xA;&#x9;when inventory_detail_table.item_desc_1 like N'%STHT-C%'  then 'STHT'&#xA;&#x9;when inventory_detail_table.item_desc_1 like N'%STHT-R%'  then 'STHT'&#xA;&#x9;when inventory_detail_table.item_desc_1 like N'%SPT50%'  then 'SPT'&#xA;&#x9;when inventory_detail_table.item_desc_1 like N'%SPT-50%'  then 'SPT'&#xA;&#x9;when inventory_detail_table.item_desc_1 like N'%Ultra%'  then 'ULTRA'&#xA;&#x9;when inventory_detail_table.item_desc_1 like N'BL%'  then 'Fitting'&#xA;&#x9;when inventory_detail_table.item_desc_1 like N'PF%'  then 'Fitting'&#xA;&#x9;when inventory_detail_table.item_desc_1 like N'SA%'  then 'SAMPLING'&#xA;END&#xA;from &#xA;(select * from inventory_category_mapping&#xA;where inventory_category_mapping.comments2  = N'通过cost center来link大小SBU,通过“描述1”link category') as a &#xA;where inventory_detail_table.[cost_center] = a.[Prod Line/cost center]&#xA;and inventory_detail_table.domain = a.Domain;&#xA;&#xA;update inventory_detail_table set category = 'Others SH-FG' where cost_center = 'HZPM' and [domain] = 'CN75' and category is null;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
