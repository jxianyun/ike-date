      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{80C1EBC5-ABD2-4A8B-B493-8DAF6ACDB080}"
          SQLTask:SqlStatementSource="update inventory_detail_table set m_final_stock_qty = Qty_on_Hand + m_open_po + m_new_po - m_demand_so_wo_qty - m_demand_fcst_qty &#xA;where item_type = 'FG' or item_type = 'FGP';&#xA;update inventory_detail_table set m_final_stock_qty = Qty_on_Hand + m_open_po + m_new_po - m_demand_so_wo_qty&#xA; where item_type = 'RAW' or item_type = 'RAW-CS';&#xA;&#xA;update inventory_detail_table set m_final_stock_qty = Qty_on_Hand,&#xA;[m+1_final_stock_qty] = Qty_on_Hand, &#xA;[m+2_final_stock_qty] = Qty_on_Hand, &#xA;[m+3_final_stock_qty] = Qty_on_Hand, &#xA;[m+4_final_stock_qty] = Qty_on_Hand, &#xA;[m+5_final_stock_qty] = Qty_on_Hand &#xA;where item_type = 'SUB-ASSY';&#xA;&#xA;update inventory_detail_table set m_final_stock_value = m_final_stock_qty  * gl_cost + wip;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
