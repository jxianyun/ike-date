      DTS:Description="执行 SQL 任务"
      DTS:DTSID="{BED32D8F-8AE3-4169-8C62-70DC18B4B3C4}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="m_new_po"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2022; © 2022 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{80C1EBC5-ABD2-4A8B-B493-8DAF6ACDB080}"
          SQLTask:SqlStatementSource="----update RM new po&#xA;/*&#xA;----LS--SEALS&#xA;UPDATE inventory_detail_table set [m_new_po]  =&#xA;CASE &#xA;&#x9;WHEN value &lt;= 0 THEN 0&#xA;&#x9;ELSE value&#xA;END&#xA;from &#xA;(select domain,item_number,item_type,m_fcst+pt_sfty_stk-qty_on_hand-m_open_po value  from&#xA;(select domain,item_number,item_type,fcst,qty_on_hand,secure_pt_mstr.pt_sfty_stk,m_open_po,&#xA;CASE &#xA;&#x9;WHEN (select day(EOMONTH(GETDATE())) - day(GETDATE())) &gt; 14&#xA;&#x9; THEN fcst/(select day(EOMONTH(GETDATE())))*(select day(EOMONTH(GETDATE())) - day(GETDATE())-14)&#xA;&#x9;ELSE 0&#xA;END m_fcst&#xA;from inventory_detail_table&#xA;LEFT JOIN&#xA;secure_pt_mstr&#xA;on inventory_detail_table.item_number = secure_pt_mstr.pt_part and inventory_detail_table.domain = secure_pt_mstr.pt_domain) as a )as b&#xA;where inventory_detail_table.Item_Number = b.item_number and inventory_detail_table.Item_Type  in ('RAW','RAW-CS')  and (&#xA;sub_sbu = 'FTS'&#xA;or sub_sbu = 'MICR' &#xA;or sub_sbu = 'FLTR' &#xA;or sub_sbu = 'MED'&#xA;or sub_sbu = 'SLS'&#xA;or sub_sbu = 'BPS'&#xA;or sub_sbu = 'APS'&#xA;or sub_sbu = 'SEAL')&#xA;and inventory_detail_table.domain = b.domain;&#xA;&#xA;-----TS/FF&#xA;UPDATE inventory_detail_table set [m_new_po] =&#xA;CASE &#xA;&#x9;WHEN value &lt;= 0 THEN 0&#xA;&#x9;ELSE value&#xA;END&#xA;from &#xA;(select domain,item_number,item_type,pt_sfty_stk-qty_on_hand-m_open_po value  from&#xA;(select domain,item_number,item_type,qty_on_hand,secure_pt_mstr.pt_sfty_stk,m_open_po&#xA;from inventory_detail_table&#xA;LEFT JOIN&#xA;secure_pt_mstr&#xA;on inventory_detail_table.item_number = secure_pt_mstr.pt_part and inventory_detail_table.domain = secure_pt_mstr.pt_domain) as a )as b&#xA;where inventory_detail_table.Item_Number = b.item_number and inventory_detail_table.Item_Type in ('RAW','RAW-CS')  and (&#xA;sub_sbu = 'COAT'&#xA;or sub_sbu = 'TS' &#xA;or sub_sbu = 'FF')&#xA;and inventory_detail_table.domain = b.domain;&#xA;&#xA;&#xA;----Bearing&#xA;UPDATE inventory_detail_table set [m_new_po] =&#xA;CASE &#xA;&#x9;WHEN value &lt;= 0 THEN 0&#xA;&#x9;ELSE value&#xA;END&#xA;from &#xA;(select domain,item_number,item_type,m_fcst+pt_sfty_stk-qty_on_hand-m_open_po value  from&#xA;(select domain,item_number,item_type,fcst,qty_on_hand,secure_pt_mstr.pt_sfty_stk,m_open_po,&#xA;CASE &#xA;&#x9;WHEN (select day(EOMONTH(GETDATE())) - day(GETDATE())) &gt; 7&#xA;&#x9; THEN fcst/(select day(EOMONTH(GETDATE())))*(select day(EOMONTH(GETDATE())) - day(GETDATE())-7)&#xA;&#x9;ELSE 0&#xA;END m_fcst&#xA;from inventory_detail_table&#xA;LEFT JOIN&#xA;secure_pt_mstr&#xA;on inventory_detail_table.item_number = secure_pt_mstr.pt_part and inventory_detail_table.domain = secure_pt_mstr.pt_domain) as a )as b&#xA;where inventory_detail_table.Item_Number = b.item_number and inventory_detail_table.Item_Type  in ('RAW','RAW-CS') and &#xA;sub_sbu = 'BEAR'&#xA;and inventory_detail_table.domain = b.domain;&#xA;*/&#xA;&#xA;&#xA;update inventory_detail_table set m_new_po = &#xA;case when&#xA;&#x9;m_demand_so_wo_qty + safety_stock - qty_on_hand -m_open_po &lt;=0 then 0&#xA;else m_demand_so_wo_qty + safety_stock - qty_on_hand -m_open_po&#xA;end&#xA;where item_type in ('RAW','RAW-CS');&#xA;&#xA;&#xA;-----------------------------------------------------------new po&#xA;----FGP&#xA;update inventory_detail_table set [m_new_po] =&#xA;CASE &#xA;&#x9;WHEN value &lt;= 0 THEN 0&#xA;&#x9;ELSE value&#xA;END&#xA;from&#xA;(select domain,item_number,item_type,[m_demand_fcst_qty]+[m_demand_so_qty]+pt_sfty_stk-qty_on_hand-[m_open_po] value  from &#xA;(select domain,item_number,item_type,[m_demand_fcst_qty],[m_demand_so_qty],qty_on_hand,[m_open_po],secure_pt_mstr.pt_sfty_stk from inventory_detail_table&#xA;LEFT JOIN&#xA;secure_pt_mstr&#xA;on inventory_detail_table.item_number = secure_pt_mstr.pt_part and inventory_detail_table.domain = secure_pt_mstr.pt_domain) as a) as b&#xA;where b.item_type = 'FGP' and inventory_detail_table.item_number = b.item_number and inventory_detail_table.domain = b.domain;&#xA;&#xA;----FG &#xA;update inventory_detail_table set [m_new_po] =&#xA;CASE &#xA;&#x9;WHEN value &lt;= 0 THEN 0&#xA;&#x9;ELSE value&#xA;END&#xA;from&#xA;(select domain,item_number,item_type,[m_demand_fcst_qty]+[m_demand_so_qty] +pt_sfty_stk-qty_on_hand value  from &#xA;(select domain,item_number,item_type,[m_demand_fcst_qty],[m_demand_so_qty],qty_on_hand,secure_pt_mstr.pt_sfty_stk from inventory_detail_table&#xA;LEFT JOIN&#xA;secure_pt_mstr&#xA;on inventory_detail_table.item_number = secure_pt_mstr.pt_part and inventory_detail_table.domain = secure_pt_mstr.pt_domain) as a) as b&#xA;where b.item_type = 'FG' and inventory_detail_table.item_number = b.item_number and inventory_detail_table.domain = b.domain;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
