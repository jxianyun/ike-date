          SQLTask:SqlStatementSource="update inventory_detail_table set wip = 0 where wip is null;&#xA;update inventory_detail_table set fcst = 0;&#xA;update inventory_detail_table set in_qty_oh = 0 where in_qty_oh is null;&#xA;update inventory_detail_table set in_qty_nonet = 0 where in_qty_nonet is null;&#xA;update inventory_detail_table set [in_supp_consign_qty] = 0 where  [in_supp_consign_qty] is null;&#xA;update inventory_detail_table set [in_cust_consign_qty] = 0 where  [in_cust_consign_qty] is null;&#xA;&#xA;update inventory_detail_table set m_demand_so_wo_qty = 0;&#xA;update inventory_detail_table set [m+1_demand_so_wo_qty] = 0;&#xA;update inventory_detail_table set [m+2_demand_so_wo_qty] = 0;&#xA;update inventory_detail_table set [m+3_demand_so_wo_qty] = 0;&#xA;update inventory_detail_table set [m+4_demand_so_wo_qty] = 0;&#xA;update inventory_detail_table set [m+5_demand_so_wo_qty] = 0;&#xA;&#xA;&#xA;update inventory_detail_table set m_demand_fcst_qty = 0 where m_demand_fcst_qty is null;&#xA;update inventory_detail_table set [m+1_demand_fcst_qty] = 0 where [m+1_demand_fcst_qty] is null ;&#xA;update inventory_detail_table set [m+2_demand_fcst_qty] = 0 where [m+2_demand_fcst_qty] is null ;&#xA;update inventory_detail_table set [m+3_demand_fcst_qty] = 0 where [m+3_demand_fcst_qty] is null ;&#xA;update inventory_detail_table set [m+4_demand_fcst_qty] = 0 where [m+4_demand_fcst_qty] is null ;&#xA;update inventory_detail_table set [m+5_demand_fcst_qty] = 0 where [m+5_demand_fcst_qty] is null ;&#xA;&#xA;update inventory_detail_table set m_demand_so_qty = 0;&#xA;update inventory_detail_table set [m+1_demand_so_qty] = 0;&#xA;update inventory_detail_table set [m+2_demand_so_qty] = 0;&#xA;update inventory_detail_table set [m+3_demand_so_qty] = 0;&#xA;update inventory_detail_table set [m+4_demand_so_qty] = 0;&#xA;update inventory_detail_table set [m+5_demand_so_qty] = 0;&#xA;&#xA;update inventory_detail_table set m_open_po = 0;&#xA;update inventory_detail_table set [m+1