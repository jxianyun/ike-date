          SQLTask:SqlStatementSource="----计算平均月耗，update到fcst字段&#xA;update inventory_detail_table set fcst = d.Qty_open_avg&#xA;from &#xA;(select tr_domain,tr_part,sum(tr_qty_chg)/12 Qty_open_avg from &#xA;(select tr_domain,tr_part,tr_type,inventory_detail_table.sub_sbu,tr_qty_chg,tr_date from [secure_tr_hist] &#xA;RIGHT JOIN&#xA;inventory_detail_table&#xA;on [secure_tr_hist].tr_part = inventory_detail_table.item_number and tr_domain = inventory_detail_table.domain) as a &#xA;where (sub_sbu = 'FTS'&#xA;or sub_sbu = 'MICR' &#xA;or sub_sbu = 'FLTR' &#xA;or sub_sbu = 'MED'&#xA;or sub_sbu = 'SLS'&#xA;or sub_sbu = 'BPS'&#xA;or sub_sbu = 'APS'&#xA;or sub_sbu = 'SEAL'&#xA;or sub_sbu = 'OS'&#xA;or sub_sbu = 'Others')&#xA;and &#xA;tr_date BETWEEN&#xA;(select convert(char(10),dateadd(month,-12,dateadd(dd,-day(GETDATE())+1,GETDATE())),120))&#xA;and &#xA;(select convert(char(10),dateadd(month,0,dateadd(dd,-day(GETDATE()),GETDATE())),120)) &#xA;and tr_type = 'ISS-WO'&#xA;GROUP BY tr_domain,tr_part&#xA;union all&#xA;select tr_domain,tr_part,sum(tr_qty_chg)/6 Qty_open_avg from &#xA;(select tr_domain,tr_part,tr_type,inventory_detail_table.sub_sbu,tr_qty_chg,tr_date from [secure_tr_hist]&#xA;RIGHT JOIN&#xA;inventory_detail_table&#xA;on [secure_tr_hist].tr_part = inventory_detail_table.item_number and tr_domain = inventory_detail_table.domain) as b &#xA;where (sub_sbu = 'COAT'&#xA;or sub_sbu = 'TS' &#xA;or sub_sbu = 'FF') &#xA;and &#xA;tr_date BETWEEN&#xA;(select convert(char(10),dateadd(month,-6,dateadd(dd,-day(GETDATE())+1,GETDATE())),120))&#xA;and &#xA;(select convert(char(10),dateadd(month,0,dateadd(dd,-day(GETDATE()),GETDATE())),120)) &#xA;and tr_type = 'ISS-WO'&#xA;GROUP BY tr_domain,tr_part&#xA;union all&#xA;select tr_domain,tr_part,sum(tr_qty_chg)/3 Qty_open_avg from &#xA;(select tr_domain,tr_part,tr_type,inventory_detail_table.sub_sbu,tr_qty_chg,tr_date from [secure_tr_hist]&#xA;RIGHT JOIN&#xA;inventory_detail_table&#xA;on [secure_tr_hist].tr_part = inventory_detail_table.item_number and tr_domain = inventory_detail_table.domain) as c&#xA;where sub_sbu = 'BEAR'&#xA;and &#xA;tr_date BETWEEN&#xA;(select convert(char(10),dateadd(month,-3,dateadd(dd,-day(GETDATE())+1,GETDATE())),120))&#xA;and &#xA;(select convert(char(10),dateadd(month,0,dateadd(dd,-day(GETDATE()),GETDATE())),120)) &#xA;and tr_type = 'ISS-WO'&#xA;GROUP BY tr_domain,tr_part) as d &#xA;where inventory_detail_table.Item_Number = d.tr_part and inventory_detail_table.domain = d.tr_domain;&#xA;&#xA;&#xA;update  inventory_detail_table set fcst = abs(fcst);&#xA;&#xA;---------------------RM for LS seals------------------------------------&#xA;UPDATE inventory_detail_table set m_demand_so_wo_qty = fcst/(select day(EOMONTH(GETDATE())))*(select day(EOMONTH(GETDATE())) - day(GETDATE())+1)&#xA;/*&#xA;CASE &#xA;&#x9;WHEN (select day(EOMONTH(GETDATE())) - day(GETDATE())) &gt; 14&#xA;&#x9;THEN fcst/(select day(EOMONTH(GETDATE())))*(select day(EOMONTH(GETDATE())) - day(GETDATE())-14)&#xA;ELSE 0&#xA;END&#xA;*/&#xA;where sub_sbu in ('FTS','MICR' ,'FLTR' ,'MED','SLS','BPS','APS','SEAL','OS') and item_type in ('RAW','RAW-CS');&#xA;&#xA;&#xA;---------------------RM for TS FF------------------------------------&#xA;UPDATE inventory_detail_table set m_demand_so_wo_qty = 0 &#xA;where Item_Type in ('RAW','RAW-CS')  and sub_sbu in ('COAT','TS' ,'FF');&#xA;&#xA;------bearings-------------&#xA;UPDATE inventory_detail_table set m_demand_so_wo_qty = fcst/(select day(EOMONTH(GETDATE())))*(select day(EOMONTH(GETDATE())) - day(GETDATE())+1)&#xA;/*&#xA;CASE &#xA;&#x9;WHEN (select day(EOMONTH(GETDATE())) - day(GETDATE())) &gt; 7&#xA;&#x9; THEN fcst/(select day(EOMONTH(GETDATE())))*(select day(EOMONTH(GETDATE())) - day(GETDATE())-7)&#xA;&#x9;ELSE 0&#xA;END&#xA;*/&#xA;where Item_Type  in ('RAW','RAW-CS') and sub_sbu = 'BEAR';&#xA;&#xA;-----------------------FG FGP----------------&#xA;update inventory_detail_table set m_demand_so_wo_qty = m_demand_so_qty&#xA;where  item_type in ('FG','FGP');&#xA;&#xA;&#xA;------------------------- 更新M+1-5 -------------------------------&#xA;update inventory_detail_table set &#xA;[m+1_demand_so_wo_qty]=fcst,&#xA;[m+2_demand_so_wo_qty]=fcst,&#xA;[m+3_demand_so_wo_qty]=fcst,&#xA;[m+4_demand_so_wo_qty]=fcst,&#xA;[m+5_demand_so_wo_qty]=fcst&#xA;where Item_Type in ('RAW','RAW-CS');" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
